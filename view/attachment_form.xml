<data>
    <xpath expr="/form/notebook" position="inside">
        <page string="Cryptolog">
            <label name="cryptolog_status"/>
            <field name="cryptolog_status"/>
            <label name="cryptolog_id"/>
            <field name="cryptolog_id"/>
            <label name="cryptolog_url"/>
            <field name="cryptolog_url" widget="url"/>
            <label name="cryptolog_data"/>
            <field name="cryptolog_data"/>
            <field name="cryptolog_signers" colspan="4"/>
            <group id="buttons" string="" col="4" colspan="4">
                <group id="free_space" colspan="2"></group>
                <button string="Request" name="cryptolog_request_transaction"/>
                <button string="Update Info" name="cryptolog_get_transaction_info"/>
            </group>
        </page>
    </xpath>
</data>
